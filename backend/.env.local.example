# Local Development Environment (SQLite)
# Database file stored in project directory
DATABASE_URL="file:./prisma/tradeline.db"

# For Hostinger Production (MySQL), use:
# DATABASE_URL="mysql://username:password@hostname:3306/database_name"

# JWT Secret (generate a strong random string for production)
JWT_SECRET=local_dev_jwt_secret_change_in_production

# TradelineSupply API (if needed)
TRADELINE_API_URL=https://api.tradelinesupply.com
TRADELINE_API_KEY=your_tradeline_api_key

# Session Secret
SESSION_SECRET=local_dev_session_secret_change_in_production

# Server Configuration
PORT=3000
NODE_ENV=development

# S3 Configuration (optional for local dev)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# S3_BUCKET_NAME=

# Stripe (optional for local dev)
# STRIPE_SECRET_KEY=
# STRIPE_WEBHOOK_SECRET=

# OpenAGI LUX Bot Configuration (for automated order fulfillment)
# Get your API key from https://developer.agiopen.org/
OAGI_API_KEY=your_openagi_api_key

# TradelineSupply Broker Credentials (for LUX Bot to login and place orders)
TRADELINE_BROKER_LOGIN_URL=https://tradelinesupply.com/login-create-an-account/?redirect_to=https://tradelinesupply.com/my-account/
TRADELINE_BROKER_LOGIN_ID=your_broker_email@example.com
TRADELINE_BROKER_PASSWORD=your_broker_password
TRADELINE_ORDERS_URL=https://tradelinesupply.com/pricing
