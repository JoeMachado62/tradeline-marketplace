          <%- contentFor('extra_javascript') %>
            <script>
              // Make project cards clickable - Owl Carousel can interfere with link clicks
              $(document).ready(function () {
                $('.project-single-boxarea3').css('cursor', 'pointer').on('click', function (e) {
                  // Don't interfere if clicking directly on a link
                  if ($(e.target).is('a') || $(e.target).closest('a').length) {
                    return true;
                  }
                  // Find the first link in this card and navigate to it
                  var link = $(this).find('.content-area a').first().attr('href');
                  if (link) {
                    window.location.href = link;
                  }
                });
              });
            </script>