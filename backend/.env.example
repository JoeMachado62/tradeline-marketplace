# Database
DATABASE_URL=postgresql://user:password@localhost:5432/tradeline_marketplace

# JWT Secret
JWT_SECRET=your-jwt-secret-here

# Environment
NODE_ENV=development
PORT=3000

# Public URL (for redirects)
PUBLIC_URL=http://localhost:3000

# AWS S3 Configuration (for document storage)
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=tradeline-documents

# TradelineSupply API (for inventory sync)
TRADELINE_API_KEY=your-tradeline-api-key
TRADELINE_API_SECRET=your-tradeline-api-secret
